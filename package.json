    "rdf-parser-rdfxml": "^0.1.0",
    "rdf-serializer-jsonld": "^0.1.0",
    "rdf-serializer-n3": "^0.1.0",
    "rdf-serializer-ntriples": "^0.1.0",
    "rdf-serializer-sparql-update": "^0.1.0",
    "rdf-store-inmemory": "^0.1.1",
    "rdf-store-ldp": "^0.1.1",
    "rdf-store-rdfstore-js": "^0.1.1",
    "rdf-store-singlegraph": "^0.1.1",
    "rdf-store-sparql": "^0.1.1"
  },
  "devDependencies": {
    "browserify": "^11.0.1",
    "exorcist": "^0.4.0",
    "uglify-js": "^2.4.24"
  }
}
